<% @merchants.each do |merchant| %>
 <ul>
  <li><%= merchant.name %></li>
  <a href="/merchants/<%= merchant.id  %>/edit">Edit</a>
  <form action="/items/<%= merchant.id %>" method="POST">
    <input type="hidden" name="_method" value="DELETE">
    <input type="submit" value="delete"/>
  </form>
 </ul>
<% end %>
